{
  "name": "ud-iam-service",
  "version": "1.0.0",
  "description": "IAM service",
  "scripts": {
    "db:generate": "npx sequelize-cli model:generate --name",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:all": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "deploy:all": "serverless deploy",
    "start:dev": "nodemon --exec \"serverless offline start\"",
    "test:coverage": "NODE_ENV=test & jest -i --silent --coverage",
    "test:local": "jest -i --coverage"
  },
  "keywords": [],
  "author": "kyel",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.327.0",
