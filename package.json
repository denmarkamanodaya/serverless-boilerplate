    "@middy/core": "^4.2.3",
    "@middy/http-header-normalizer": "^4.2.6",
    "@middy/http-json-body-parser": "^4.2.3",
    "auth0": "^4.15.0",
    "axios": "^1.3.1",
    "config-dug": "^1.7.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "dynamodb-toolbox": "^0.8.5",
    "enum": "^3.0.4",
    "g": "^2.0.1",
    "http-status": "^1.5.3",
    "joi": "^17.3.0",
    "jose": "^5.9.6",
    "moment": "^2.29.4",
    "moment-countdown": "^0.0.3",
    "mysql2": "^3.1.0",
    "otp-gen-agent": "^1.1.2",
    "otplib": "^12.0.1",
    "sequelize": "^6.19.0",
    "serverless": "^3.39.0",
    "serverless-plugin-include-dependencies": "^6.1.1",
    "thirty-two": "^1.0.2",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
